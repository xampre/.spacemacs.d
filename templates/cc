/*
  `(file-name-nondirectory (buffer-file-name))`

  Created: `(when (require 'time-stamp nil t)
              (time-stamp-string "%:dth %:b, %:y (%3a.)"))`
  Author: `(user-login-name)`
*/

#include <iostream>
#include <vector>
`(let ((header
        (concat (file-name-sans-extension (file-name-nondirectory (buffer-file-name))) ".h"))
       (fmt "#include \"%s\""))
  (if (file-exists-p header)
      (format fmt header)
    (format (concat "// " fmt) header)))`
using namespace std;

$0
